                                                                        <tbody>
                                                                            <tr valign="top">
                                                                                <td colspan="3" align="left" class="headertext_red">Summon</td>
                                                                            </tr>
                                                                            <tr valign="top">
                                                                                <td align="left" width="120"><strong>Unable To Use</strong></td>
                                                                                <td align="left" width="10">•</td>
                                                                                <td>Calling Friend Scroll.</td>
                                                                            </tr>
                                                                            <tr valign="top">
                                                                                <td align="left">&nbsp;</td>
                                                                                <td align="left">•</td>
                                                                                <td>Enchant Scroll (STR, DEX, INT, MAGIC, HP and ARMOR).</td>
                                                                            </tr>
                                                                            <tr valign="top">
                                                                                <td align="left">&nbsp;</td>
                                                                                <td align="left">•</td>
                                                                                <td>Transformation Scroll.</td>
                                                                            </tr>
                                                                            <tr valign="top">
                                                                                <td align="left" width="120"><strong>Able To Use</strong></td>
                                                                                <td align="left" width="10">•</td>
                                                                                <td>Summons friend skill and call party skill.</td>
                                                                            </tr>
